<div class="ui container">
	<div class = "ui celled grid">
		<div class="row">
			<h3>Lista de Senadores</h3>
		</div>
		<div class="row">
			<ul>
				{{#each list.data}}
					<li><a href="/aecio">{{this.name}}</a> - {{this.party}} ({{this.state}})</li>
				{{else}}
					no data
				{{/each}}
			</ul>
		</div>
	</div>
</div>
